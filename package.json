{
  "name": "dot-values2",
  "version": "2.0.7",
  "description": "A set of object mutators with dot paths and one neat addition...",
  "keywords": [
    "dot",
    "dots",
    "path",
    "set-value",
    "get-value",
    "unset-value",
    "has-value",
    "lodash",
    "prop",
    "props",
    "dot-prop",
    "property",
    "properties",
    "typesctipt",
    "star",
    "dot-wild",
    "wild",
    "object"
  ],
  "homepage": "https://github.com/bluelovers/dot-values#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/dot-values/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/dot-values.git"
  },
  "license": "MIT",
  "author": "bajankristof",
  "main": "lib/index",
  "module": "esm/index",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "yarn run test -- --coverage",
    "test": "jest",
    "test:jest": "jest --passWithNoTests",
    "test:jest:coverage": "yarn run test:jest -- --coverage",
    "test:jest:snapshot": "yarn run test:jest -- -u",
    "test:snapshot": "yarn run test -- -u",
    "test:tsd": "ynpx tsd",
    "build:tsc": "yarn run tsc:default && yarn run tsc:esm",
    "npm:publish:bump": "yarn-tool version && npm publish && yarn run postpublishOnly",
    "ws:fix-all": "yarn-tool fix-all  --overwriteHostedGitInfo",
    "ws:fix-all:resetStaticFiles": "yarn run ws:fix-all -- --resetStaticFiles",
    "preversion": "yarn run test && yarn run build:tsc",
    "version": "echo version",
    "prepublishOnly": "yarn run preversion",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "postpublishOnly": "yarn run postpublish",
    "benchmark": "tsx ./test/benchmark",
    "sort-package-json": "yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json",
    "tsc:showConfig": "ynpx get-current-tsconfig -p"
  },
  "dependencies": {
    "escape-split": "^1.0.5",
    "get-value": "^3.0.1",
    "has-value": "^2.0.2",
    "isobject": "^4.0.0",
    "set-value": "^4.1.0",
    "split-string": "^6.1.0",
    "ts-type": "^3.0.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.35",
    "@types/benchmark": "^2.1.5",
    "@types/chai": "^4.3.12",
    "@types/get-value": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/jsonpath": "^0.2.4",
    "@types/microtime": "^2.1.2",
    "@types/node": "^20.11.24",
    "@types/set-value": "^4.0.3",
    "benchmark": "^2.1.4",
    "benny": "^3.7.1",
    "dot-prop": "^8.0.2",
    "dot-wild": "^3.0.1",
    "dotprop": "^1.2.1",
    "jest": "^29.7.0",
    "jsonpath": "^1.1.1",
    "microtime": "^3.1.1",
    "ts-jest": "^29.1.2",
    "unset-value": "github:bluelovers/unset-value"
  }
}
