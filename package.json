{
  "name": "dot-values2",
  "version": "2.0.5",
  "description": "A set of object mutators with dot paths and one neat addition...",
  "keywords": [
    "dot",
    "dots",
    "path",
    "set-value",
    "get-value",
    "unset-value",
    "has-value",
    "lodash",
    "prop",
    "props",
    "dot-prop",
    "property",
    "properties",
    "typesctipt",
    "star",
    "dot-wild",
    "wild",
    "object"
  ],
  "homepage": "https://github.com/bluelovers/dot-values#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/dot-values/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/dot-values.git"
  },
  "license": "MIT",
  "author": "bajankristof",
  "main": "lib/index",
  "module": "esm/index",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "npm:publish:bump": "yarn-tool version && yarn run preversion && npm publish && yarn run postpublishOnly",
    "test": "jest",
    "build:tsc": "yarn run tsc:default && yarn run tsc:esm",
    "npm:publish": "npm publish",
    "lerna:publish:yes": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "preversion": "yarn run build:tsc",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "postpublishOnly": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "benchmark": "npx ts-node ./test/benchmark",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "escape-split": "^1.0.5",
    "get-value": "^3.0.1",
    "has-value": "^2.0.2",
    "isobject": "^4.0.0",
    "set-value": "^4.0.0",
    "split-string": "^6.1.0",
    "ts-type": "^1.2.35",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.22",
    "@types/benchmark": "^2.1.0",
    "@types/chai": "^4.2.19",
    "@types/get-value": "^3.0.1",
    "@types/jest": "^26.0.23",
    "@types/jsonpath": "^0.2.0",
    "@types/microtime": "^2.1.0",
    "@types/node": "^16.0.0",
    "@types/set-value": "^4.0.0",
    "benchmark": "^2.1.4",
    "benny": "^3.6.15",
    "dot-prop": "^6.0.1",
    "dot-wild": "^3.0.1",
    "dotprop": "^1.2.1",
    "jest": "^27.0.6",
    "jsonpath": "^1.1.1",
    "microtime": "^3.0.0",
    "ts-jest": "^27.0.3",
    "unset-value": "github:bluelovers/unset-value"
  }
}
