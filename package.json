{
  "name": "dot-values2",
  "version": "1.0.4",
  "description": "A set of object mutators with dot paths and one neat addition...",
  "keywords": [
    "dot",
    "dots",
    "path",
    "set-value",
    "get-value",
    "unset-value",
    "has-value",
    "lodash",
    "prop",
    "props",
    "dot-prop",
    "property",
    "properties",
    "typesctipt",
    "star",
    "object"
  ],
  "homepage": "https://github.com/bluelovers/dot-values#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/dot-values/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/dot-values.git"
  },
  "license": "MIT",
  "author": "bajankristof",
  "main": "lib/index",
  "module": "esm/index",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build:tsc": "yarn run tsc:default && yarn run tsc:esm",
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"publish new version\" .",
    "prepublishOnly": "yarn run build:tsc && yarn run ncu && yarn run sort-package-json && yarn run test",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish": "git commit -m \"publish new version\" .",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "npx mocha --debug --require ts-node/register !(node_modules)/**/*.{test,spec}.{ts,tsx}",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "escape-split": "^1.0.5",
    "get-value": "^3.0.1",
    "has-value": "^2.0.2",
    "set-value": "^3.0.1",
    "ts-type": "^1.2.12"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.13",
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.2.7",
    "@types/faker": "^4.1.9",
    "@types/get-value": "^3.0.1",
    "@types/jsonpath": "^0.2.0",
    "@types/microtime": "^2.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.6",
    "@types/set-value": "^2.0.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "faker": "^4.1.0",
    "false": "^0.0.4",
    "jsonpath": "^1.0.2",
    "microtime": "^3.0.0",
    "mocha": "^7.0.0",
    "split-string": "^6.1.0",
    "unset-value": "bluelovers/unset-value"
  }
}
